      "tagName": "@alias",
      "syntaxKind": "block",
      "allowMultiple": false
    },
    {
      "tagName": "@category",
      "syntaxKind": "block",
      "allowMultiple": false
    },
    {
      "tagName": "@default",
      "syntaxKind": "block",
      "allowMultiple": false
    },
    {
      "tagName": "@entrypointCategory",
      "syntaxKind": "block",
      "allowMultiple": false
    },
    {
      "tagName": "@since",
      "syntaxKind": "block",
      "allowMultiple": false
    },
    {
      "tagName": "@docGroup",
      "syntaxKind": "block",
      "allowMultiple": false
    }
  ],

  "supportForTags": {
    "@alias": true,
    "@category": true,
    "@default": true,
    "@entrypointCategory": true,
    "@since": true,
    "@docGroup": true
  }
